{
	"[javascript]": {
		"editor.codeActionsOnSave": {
			"source.organizeImports": "explicit"
		},
		"editor.defaultFormatter": "biomejs.biome",
		"editor.formatOnSave": true
	},
	"[tailwindcss]": {
		"editor.defaultFormatter": "MishaLestev.tailwind-css-language-mode-formatter",
		"editor.formatOnSave": true
	},
	"[javascriptreact]": {
		"editor.codeActionsOnSave": {
			"source.organizeImports": "explicit"
		},
		"editor.defaultFormatter": "biomejs.biome",
		"editor.formatOnSave": true
	},
	"[json]": {
		"editor.defaultFormatter": "biomejs.biome",
		"editor.formatOnSave": true,
		"editor.quickSuggestions": {
			"strings": true
		},
		"editor.suggest.insertMode": "replace"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "biomejs.biome",
		"editor.formatOnSave": true,
		"editor.quickSuggestions": {
			"strings": true
		},
		"editor.suggest.insertMode": "replace"
	},
	"[typescript]": {
		"editor.codeActionsOnSave": {
			"source.organizeImports": "explicit"
		},
		"editor.defaultFormatter": "biomejs.biome",
		"editor.formatOnSave": true
	},
	"[typescriptreact]": {
		"editor.codeActionsOnSave": {
			"source.organizeImports": "explicit"
		},
		"editor.defaultFormatter": "biomejs.biome",
		"editor.formatOnSave": true
	},
	"cSpell.words": [
		"bezkrovny",
		"biomejs",
		"clsx",
		"commitlint",
		"Configurator",
		"divs",
		"EISDIR",
		"esbuild",
		"estree",
		"mssim",
		"nocheck",
		"pathe",
		"pixelmatch",
		"pngjs",
		"pnpx",
		"repobuddy",
		"Resizer",
		"shipit",
		"Snyk",
		"ssim",
		"testid",
		"tsup",
		"vite",
		"Vitest"
	],
	"editor.codeActionsOnSave": {
		"source.organizeImports.biome": "explicit"
	},
	"files.associations": {
		"tailwind.css": "tailwindcss"
	},
	"git-graph.dialog.general.referenceInputSpaceSubstitution": "Hyphen",
	"material-icon-theme.files.associations": {
		"*.accept.ts": "test-ts",
		"*.excalidraw": "maven",
		"*.integrate.ts": "test-ts",
		"*.spec.node.ts": "test-ts",
		"*.study.ts": "test-ts",
		"*.system.ts": "test-ts",
		"*.unit.node.ts": "test-ts",
		"*.unit.ts": "test-ts",
		"vitest.config.browser.ts": "vitest",
		"vitest.config.node.ts": "vitest",
		"vitest.config.playwright.ts": "vitest",
		"vitest.config.webdriverio.ts": "vitest",
		"vitest.setup.ts": "vitest",
		"vitest.setup.playwright.ts": "vitest",
		"vitest.setup.webdriverio.ts": "vitest"
	},
	"typescript.updateImportsOnFileMove.enabled": "always",
	"typescript.updateImportsOnPaste.enabled": true,
	"typescript.preferences.importModuleSpecifierEnding": "js",
	"typescript.preferences.includePackageJsonAutoImports": "on",
	"typescript.tsdk": "node_modules/typescript/lib",
	"typescript.tsserver.web.projectWideIntellisense.enabled": true,
	"files.exclude": {
		"node_modules/.pnpm/node_modules/testcase-workspace/node_modules/tailwindcss/stubs/**/*": false,
		"node_modules/.pnpm/node_modules/testcase-themes/node_modules/tailwindcss/stubs/**/*": false,
		"node_modules/.pnpm/tailwindcss@3.4.17/node_modules/tailwindcss/stubs/**/*": false,
		"**/.DS_Store": true,
		"**/logs": true,
		"**/*.log": true,
		"**/npm-debug.log*": true,
		"**/pids": true,
		"**/*.pid": true,
		"**/*.seed": true,
		"**/lib-cov": true,
		"**/coverage": true,
		"**/.nyc_output": true,
		"**/.progress": true,
		"**/.grunt": true,
		"**/.lock-wscript": true,
		"build/Release": true,
		"**/jspm_packages": true,
		"**/node_modules": true,
		"**/.npm": true,
		"**/.node_repl_history": true,
		"**/.idea": true,
		"**/.pnp.*": true,
		".yarn/*": true,
		".yarn/patches": false,
		".yarn/plugins": false,
		".yarn/releases": false,
		".yarn/sdks": false,
		".yarn/versions": false,
		"**/.turbo": true,
		"**/vite*.timestamp*": true,
		"**/__screenshots__": true,
		"**/__snapshots__/*/__diff_output__": true,
		"**/__snapshots__/*/__results__": true,
		"**/__vis__/__diff_output__": true,
		"**/__vis__/**/__diffs__": true,
		"**/__vis__/**/__results__": true,
		"**/__vis__/local": true,
		"**/__snapshots__/__diff_output__": true,
		"**/__snapshots__/__results__": true,
		"**/__snapshots__/local": true,
		"**/*.tsbuildinfo": true,
		"**/build-storybook.log": true,
		"**/dist": true,
		"**/esm": true,
		"**/storybook-static": true,
		"testcases/workspace/node_modules/tailwindcss/stubs/**/*": false,
		"testcases/themes/node_modules/tailwindcss/stubs/**/*": false
	}
}
